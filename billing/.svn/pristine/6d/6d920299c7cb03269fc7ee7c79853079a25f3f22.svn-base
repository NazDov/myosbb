package ua.ssmt.softserveinc.com.lvklegals.webservice.additional.domain;

import java.util.Date;

/**
 * Created by vostap on 8/4/2015.
 */
public class CalcInvoicePDV {
     private Long id;
     private Date period;
     private ContraAgent contraAgentID;
     private Double totalInvoiceWithoutPDV;
     private Double totalInoiceWithPDV;
     private Double sumPDV;
     private ServicePayment servicePayment;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Date getPeriod() {
        return period;
    }

    public void setPeriod(Date period) {
        this.period = period;
    }

    public ContraAgent getContraAgentID() {
        return contraAgentID;
    }

    public void setContraAgentID(ContraAgent contraAgentID) {
        this.contraAgentID = contraAgentID;
    }

    public Double getTotalInvoiceWithoutPDV() {
        return totalInvoiceWithoutPDV;
    }

    public void setTotalInvoiceWithoutPDV(Double totalInvoiceWithoutPDV) {
        this.totalInvoiceWithoutPDV = totalInvoiceWithoutPDV;
    }

    public Double getTotalInoiceWithPDV() {
        return totalInoiceWithPDV;
    }

    public void setTotalInoiceWithPDV(Double totalInoiceWithPDV) {
        this.totalInoiceWithPDV = totalInoiceWithPDV;
    }

    public Double getSumPDV() {
        return sumPDV;
    }

    public void setSumPDV(Double sumPDV) {
        this.sumPDV = sumPDV;
    }

    public ServicePayment getServicePayment() {
        return servicePayment;
    }

    public void setServicePayment(ServicePayment servicePayment) {
        this.servicePayment = servicePayment;
    }
}
