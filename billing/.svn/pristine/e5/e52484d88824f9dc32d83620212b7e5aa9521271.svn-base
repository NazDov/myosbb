package ua.ssmt.softserveinc.com.lvklegals.webservice.additional.exeptions.datasource;

/**
 * Created by vostap on 23.08.2014.
 */
public class DataSourceLoadingConfigurationException extends DataSourceException {
    public DataSourceLoadingConfigurationException() {
    }

    public DataSourceLoadingConfigurationException(String message) {
        super(message);
    }

    public DataSourceLoadingConfigurationException(String message, Throwable cause) {
        super(message, cause);
    }

    public DataSourceLoadingConfigurationException(Throwable cause) {
        super(cause);
    }

    public DataSourceLoadingConfigurationException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
        super(message, cause, enableSuppression, writableStackTrace);
    }
}
