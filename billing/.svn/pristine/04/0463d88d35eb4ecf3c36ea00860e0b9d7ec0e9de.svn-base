package ua.ssmt.softserveinc.com.lvklegals.webservice.additional.domain;

import java.util.Date;

/**
 * Created by vostap on 22.09.2014.
 */
public class Controller {
    private Long id;
    private String name;
    private Long registerID;
    private SubRoute subRoute;
    private Date period;

    public Controller() {

        this.subRoute = new SubRoute();
    }

    public Controller(Long id) {
        this();
        this.id = id;
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Long getRegisterID() {
        return registerID;
    }

    public void setRegisterID(Long registerID) {
        this.registerID = registerID;
    }

    public SubRoute getSubRoute() {
        return subRoute;
    }

    public void setSubRoute(SubRoute subRoute) {
        this.subRoute = subRoute;
    }

    @Override
    public String toString() {
        return "Controller{" +
                "id=" + id +
                ", name='" + name + '\'' +
                ", registerID=" + registerID +
                ", subRoute=" + subRoute +
                '}';
    }

    public Date getPeriod() {
        return period;
    }

    public void setPeriod(Date period) {
        this.period = period;
    }
}
