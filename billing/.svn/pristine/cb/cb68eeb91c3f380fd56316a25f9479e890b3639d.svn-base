public class SaldoDepartmentDetails {
    private Long id;
    private Long saldoDepartmentID;
    private Double sumCorrectionOfRealize;
    private Double sumInvoiceByLKP;
    private Double sumPaymentDepartment;
    private Double sumInnerOborot;
    private Double sumComiss;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Long getSaldoDepartmentID() {
        return saldoDepartmentID;
    }

    public void setSaldoDepartmentID(Long saldoDepartmentID) {
        this.saldoDepartmentID = saldoDepartmentID;
    }

    public Double getSumCorrectionOfRealize() {
        return sumCorrectionOfRealize;
    }

    public void setSumCorrectionOfRealize(Double sumCorrectionOfRealize) {
        this.sumCorrectionOfRealize = sumCorrectionOfRealize;
    }

    public Double getSumInvoiceByLKP() {
        return sumInvoiceByLKP;
    }

    public void setSumInvoiceByLKP(Double sumInvoiceByLKP) {
        this.sumInvoiceByLKP = sumInvoiceByLKP;
    }

    public Double getSumPaymentDepartment() {
        return sumPaymentDepartment;
    }

    public void setSumPaymentDepartment(Double sumPaymentDepartment) {
        this.sumPaymentDepartment = sumPaymentDepartment;
    }

    public Double getSumInnerOborot() {
        return sumInnerOborot;
    }

    public void setSumInnerOborot(Double sumInnerOborot) {
        this.sumInnerOborot = sumInnerOborot;
    }

    public Double getSumComiss() {
        return sumComiss;
    }

    public void setSumComiss(Double sumComiss) {
        this.sumComiss = sumComiss;
    }
}
